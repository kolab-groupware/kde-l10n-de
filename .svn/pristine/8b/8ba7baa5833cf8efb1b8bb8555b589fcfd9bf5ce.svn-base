#. Tag: title
#: man-kpat.6.docbook:110
#, no-c-format
msgid "Qt Options"
msgstr ""

#. Tag: option
#: man-kpat.6.docbook:114
#, no-c-format
msgid "--display <replaceable>displayname</replaceable>"
msgstr ""

#. Tag: para
#: man-kpat.6.docbook:116
#, no-c-format
msgid "Use the X-server display <emphasis>displayname</emphasis>."
msgstr ""

#. Tag: option
#: man-kpat.6.docbook:121
#, no-c-format
msgid "--session <replaceable>sessionId</replaceable>"
msgstr ""

#. Tag: para
#: man-kpat.6.docbook:123
#, no-c-format
msgid ""
"Restore the application for the given <replaceable>sessionId</replaceable>."
msgstr ""

#. Tag: option
#: man-kpat.6.docbook:130
#, no-c-format
msgid "--cmap"
msgstr ""

#. Tag: para
#: man-kpat.6.docbook:132
#, no-c-format
msgid ""
"Causes the application to install a private color map on an 8-bit display."
msgstr ""

#. Tag: option
#: man-kpat.6.docbook:140
#, no-c-format
msgid "--ncols <replaceable>count</replaceable>"
msgstr ""

#. Tag: para
#: man-kpat.6.docbook:142
#, no-c-format
msgid ""
"Limits the number of colors allocated in the color cube on a 8-bit display, if "
"the application is using the <methodname>QApplication::ManyColor</methodname> "
"color specification."
msgstr ""

#. Tag: option
#: man-kpat.6.docbook:151
#, no-c-format
msgid "--nograb"
msgstr ""

#. Tag: para
#: man-kpat.6.docbook:153
#, no-c-format
msgid "tells Qt to never grab the mouse or the keyboard."
msgstr ""

#. Tag: option
#: man-kpat.6.docbook:157
#, no-c-format
msgid "--dograb"
msgstr ""

#. Tag: para
#: man-kpat.6.docbook:159
#, no-c-format
msgid ""
"running under a debugger can cause an implicit <option>-nograb</option>"
", use <option>-dograb</option> to override."
msgstr ""

#. Tag: option
#: man-kpat.6.docbook:167
#, no-c-format
msgid "--sync"
msgstr ""

#. Tag: para
#: man-kpat.6.docbook:169
#, no-c-format
msgid "switches to synchronous mode for debugging."
msgstr ""

#. Tag: term
#: man-kpat.6.docbook:173
#, no-c-format
msgid ""
"<option>--fn</option>, <option>--font <replaceable>fontname</replaceable>"
"</option>"
msgstr ""

#. Tag: para
#: man-kpat.6.docbook:175
#, no-c-format
msgid "defines the application font."
msgstr ""

#. Tag: term
#: man-kpat.6.docbook:179
#, no-c-format
msgid ""
"<option>--bg</option>, <option>--background <replaceable>color</replaceable>"
"</option>"
msgstr ""

#. Tag: para
#: man-kpat.6.docbook:181
#, no-c-format
msgid ""
"sets the default background color and an application palette (light and dark "
"shades are calculated)."
msgstr ""

#. Tag: term
#: man-kpat.6.docbook:189
#, no-c-format
msgid ""
"<option>--fg</option>,<option>--foreground <replaceable>color</replaceable>"
"</option>"
msgstr ""

#. Tag: para
#: man-kpat.6.docbook:191
#, no-c-format
msgid "sets the default foreground color."
msgstr ""

#. Tag: term
#: man-kpat.6.docbook:195
#, no-c-format
msgid ""
"<option>--btn</option>, <option>--button <replaceable>color</replaceable>"
"</option>"
msgstr ""

#. Tag: para
#: man-kpat.6.docbook:197
#, no-c-format
msgid "sets the default button color."
msgstr ""

#. Tag: option
#: man-kpat.6.docbook:201
#, no-c-format
msgid "--name <replaceable>name</replaceable>"
msgstr ""

#. Tag: para
#: man-kpat.6.docbook:203
#, no-c-format
msgid "sets the application name."
msgstr ""

#. Tag: option
#: man-kpat.6.docbook:207
#, no-c-format
msgid "--title <replaceable>title</replaceable>"
msgstr ""

#. Tag: para
#: man-kpat.6.docbook:209
#, no-c-format
msgid "sets the application title (caption)."
msgstr ""

#. Tag: option
#: man-kpat.6.docbook:213
#, no-c-format
msgid "--visual <replaceable>TrueColor</replaceable>"
msgstr ""

#. Tag: para
#: man-kpat.6.docbook:215
#, no-c-format
msgid "forces the application to use a TrueColor visual on an 8-bit display."
msgstr ""

#. Tag: option
#: man-kpat.6.docbook:222
#, no-c-format
msgid "--inputstyle"
msgstr ""

#. Tag: para
#: man-kpat.6.docbook:224
#, no-c-format
msgid ""
"sets XIM (X Input Method) input style. Possible values are onthespot, "
"overthespot, offthespot and root."
msgstr ""

#. Tag: option
#: man-kpat.6.docbook:232
#, no-c-format
msgid "--im <replaceable>XIM server</replaceable>"
msgstr ""

#. Tag: para
#: man-kpat.6.docbook:234
#, no-c-format
msgid "set XIM server."
msgstr ""

#. Tag: option
#: man-kpat.6.docbook:238
#, no-c-format
msgid "--noxim"
msgstr ""

#. Tag: para
#: man-kpat.6.docbook:240
#, no-c-format
msgid "disable XIM."
msgstr ""

#. Tag: option
#: man-kpat.6.docbook:244
#, no-c-format
msgid "--reverse"
msgstr ""

#. Tag: para
#: man-kpat.6.docbook:246
#, no-c-format
msgid "mirrors the whole layout of widgets."
msgstr ""

#. Tag: option
#: man-kpat.6.docbook:250
#, no-c-format
msgid "--stylesheet <replaceable>file.qss</replaceable>"
msgstr ""

#. Tag: para
#: man-kpat.6.docbook:252
#, no-c-format
msgid "applies the Qt stylesheet to the application widgets."
msgstr ""

#. Tag: option
#: man-kpat.6.docbook:256
#, no-c-format
msgid "--graphicssystem <replaceable>system</replaceable>"
msgstr ""

#. Tag: para
#: man-kpat.6.docbook:258
#, no-c-format
msgid ""
"use a different graphics system instead of the default one, options are <quote>"
"raster</quote> and <quote>opengl</quote> (experimental)."
msgstr ""

#. Tag: title
#: man-kpat.6.docbook:266
#, no-c-format
msgid "KDE Options"
msgstr ""

#. Tag: option
#: man-kpat.6.docbook:269
#, no-c-format
msgid "--caption <replaceable>caption</replaceable>"
msgstr ""

#. Tag: para
#: man-kpat.6.docbook:271
#, no-c-format
msgid "Use <emphasis>caption</emphasis> as name in the titlebar."
msgstr ""

#. Tag: option
#: man-kpat.6.docbook:276
#, no-c-format
msgid "--icon <replaceable>icon</replaceable>"
msgstr ""

#. Tag: para
#: man-kpat.6.docbook:278
#, no-c-format
msgid "Use <emphasis>icon</emphasis> as the application icon."
msgstr ""

#. Tag: option
#: man-kpat.6.docbook:283
#, no-c-format
msgid "--config <replaceable>filename</replaceable>"
msgstr ""

#. Tag: para
#: man-kpat.6.docbook:285
#, no-c-format
msgid "Use alternative configuration file."
msgstr ""

#. Tag: option
#: man-kpat.6.docbook:292
#, no-c-format
msgid "--nocrashhandler"
msgstr ""

#. Tag: para
#: man-kpat.6.docbook:294
#, no-c-format
msgid "Disable crash handler, to get core dumps."
msgstr ""

#. Tag: option
#: man-kpat.6.docbook:299
#, no-c-format
msgid "--waitforwm"
msgstr ""

#. Tag: para
#: man-kpat.6.docbook:301
#, no-c-format
msgid "Waits for a WM_NET compatible windowmanager."
msgstr ""

#. Tag: option
#: man-kpat.6.docbook:308
#, no-c-format
msgid "--style <replaceable>style</replaceable>"
msgstr ""

#. Tag: para
#: man-kpat.6.docbook:310
#, no-c-format
msgid "sets the application &GUI; style."
msgstr ""

#. Tag: option
#: man-kpat.6.docbook:315
#, no-c-format
msgid "--geometry <replaceable>geometry</replaceable>"
msgstr ""

#. Tag: para
#: man-kpat.6.docbook:317
#, no-c-format
msgid ""
"sets the client geometry of the main widget - see the manual page <ulink "
"url=\"man:/X\">man:/X</ulink> for the argument format (usually "
"WidthxHeight+XPos+YPos)."
msgstr ""

#. Tag: title
#: man-kpat.6.docbook:329
#, no-c-format
msgid "Options"
msgstr ""

#. Tag: term
#: man-kpat.6.docbook:333
#, no-c-format
msgid "<option>--solvegame</option> <replaceable> file</replaceable>"
msgstr ""

#. Tag: para
#: man-kpat.6.docbook:335
#, no-c-format
msgid "Try to find a solution to the given savegame"
msgstr ""

#. Tag: term
#: man-kpat.6.docbook:340
#, no-c-format
msgid "<option>--solve</option> <replaceable> num</replaceable>"
msgstr ""

#. Tag: para
#: man-kpat.6.docbook:342
#, no-c-format
msgid "Dealer to solve (debug)"
msgstr ""

#. Tag: term
#: man-kpat.6.docbook:347
#, no-c-format
msgid "<option>--start</option> <replaceable> num</replaceable>"
msgstr ""

#. Tag: para
#: man-kpat.6.docbook:349
#, no-c-format
msgid "Game range start (default 0:INT_MAX)"
msgstr ""

#. Tag: term
#: man-kpat.6.docbook:354
#, no-c-format
msgid "<option>--end</option> <replaceable> num</replaceable>"
msgstr ""

#. Tag: para
#: man-kpat.6.docbook:356
#, no-c-format
msgid "Game range end (default start:start if start given)"
msgstr ""

#. Tag: term
#: man-kpat.6.docbook:361
#, no-c-format
msgid "<option>--gametype</option> <replaceable> game</replaceable>"
msgstr ""

#. Tag: para
#: man-kpat.6.docbook:363
#, no-c-format
msgid ""
"Skip the selection screen and load a particular game type. Valid values are: "
"acesup, fortyeight, freecell, golf, grandfather, grandfathersclock, gypsy, "
"klondike, mod3, simplesimon, spider, yukon"
msgstr ""

