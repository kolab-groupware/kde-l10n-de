#, no-c-format
msgid "The Main &appname; Window"
msgstr ""

#. Tag: para
#: menus.docbook:15
#, no-c-format
msgid ""
"Each menu item is discussed below. When there is a keyboard shortcut that "
"performs a menu item function, the default shortcut is listed with the menu "
"item."
msgstr ""

#. Tag: title
#: menus.docbook:20
#, no-c-format
msgid "The <guimenu>File</guimenu> Menu"
msgstr ""

#. Tag: menuchoice
#: menus.docbook:27
#, no-c-format
msgid "<guimenu>File</guimenu> <guimenuitem>New</guimenuitem>"
msgstr ""

#. Tag: para
#: menus.docbook:34
#, no-c-format
msgid ""
"This command <action>creates a new collection</action>"
". &appname; supports <link linkend=\"collection-types\">"
"12 default collection types</link> along with an empty user-defined custom "
"collection."
msgstr ""

#. Tag: menuchoice
#: menus.docbook:40
#, no-c-format
msgid ""
"<shortcut> <keycombo>&Ctrl;<keycap>O</keycap></keycombo> </shortcut> <guimenu>"
"File</guimenu> <guimenuitem>Open...</guimenuitem>"
msgstr ""

#. Tag: para
#: menus.docbook:50
#, no-c-format
msgid "This command <action>opens a &appname; file</action>."
msgstr ""

#. Tag: menuchoice
#: menus.docbook:56
#, no-c-format
msgid "<guimenu>File</guimenu> <guimenuitem>Open Recent</guimenuitem>"
msgstr ""

#. Tag: para
#: menus.docbook:63
#, no-c-format
msgid ""
"This command <action>opens a file</action> from a submenu that contains a list "
"of recently opened files."
msgstr ""

#. Tag: menuchoice
#: menus.docbook:70
#, no-c-format
msgid ""
"<shortcut> <keycombo>&Ctrl;<keycap>S</keycap></keycombo> </shortcut> <guimenu>"
"File</guimenu> <guimenuitem>Save...</guimenuitem>"
msgstr ""

#. Tag: para
#: menus.docbook:80
#, no-c-format
msgid ""
"This command <action>saves the collection</action>. If the file is <emphasis>"
"Untitled</emphasis> then <guimenuitem>Save...</guimenuitem> "
"is equivalent to <guimenuitem>Save As</guimenuitem>. It is only enabled when "
"the collection has been modified."
msgstr ""

#. Tag: menuchoice
#: menus.docbook:89
#, no-c-format
msgid "<guimenu>File</guimenu> <guimenuitem>Save As...</guimenuitem>"
msgstr ""

#. Tag: para
#: menus.docbook:96
#, no-c-format
msgid "This command <action>saves the collection to a new file</action>."
msgstr ""

#. Tag: menuchoice
#: menus.docbook:102
#, no-c-format
msgid ""
"<shortcut> <keycombo>&Ctrl;<keycap>P</keycap></keycombo> </shortcut> <guimenu>"
"File</guimenu> <guimenuitem>Print...</guimenuitem>"
msgstr ""

#. Tag: para
#: menus.docbook:112
#, no-c-format
msgid ""
"This command <action>prints the collection</action>. Printing options may be "
"set in the <interface>Configuration Dialog</interface>."
msgstr ""

#. Tag: menuchoice
#: menus.docbook:118
#, no-c-format
msgid "<guimenu>File</guimenu> <guimenuitem>Import</guimenuitem>"
msgstr ""

#. Tag: para
#: menus.docbook:125
#, no-c-format
msgid ""
"This command <action>imports data from another file</action>"
". &appname; can import data from a number of <link linkend=\"importing\">"
"other formats</link>."
msgstr ""

#. Tag: menuchoice
#: menus.docbook:131
#, no-c-format
msgid "<guimenu>File</guimenu> <guimenuitem>Export</guimenuitem>"
msgstr ""

#. Tag: para
#: menus.docbook:138
#, no-c-format
msgid ""
"This command <action>exports the collection to another format</action>"
". &appname; can export data to a number of <link linkend=\"exporting\">"
"other formats</link>."
msgstr ""

#. Tag: menuchoice
#: menus.docbook:144
#, no-c-format
msgid ""
"<shortcut> <keycombo>&Ctrl;<keycap>Q</keycap></keycombo> </shortcut> <guimenu>"
"File</guimenu> <guimenuitem>Quit</guimenuitem>"
msgstr ""

#. Tag: para
#: menus.docbook:154
#, no-c-format
msgid ""
"This command <action>closes the &appname; window</action>"
". If the collection has unsaved changes, you will be prompted to save them "
"before continuing."
msgstr ""

#. Tag: title
#: menus.docbook:164
#, no-c-format
msgid "The <guimenu>Edit</guimenu> Menu"
msgstr ""

#. Tag: menuchoice
#: menus.docbook:172
#, no-c-format
msgid ""
"<shortcut> <keycombo>&Ctrl;<keycap>Z</keycap></keycombo> </shortcut> <guimenu>"
"Edit</guimenu> <guimenuitem>Undo</guimenuitem>"
msgstr ""

#. Tag: para
#: menus.docbook:182
#, no-c-format
msgid ""
"This command <action>undoes the previous action</action>"
". Not all actions are supported by Undo."
msgstr ""

#. Tag: menuchoice
#: menus.docbook:188
#, no-c-format
msgid ""
"<shortcut> <keycombo>&Ctrl;&Shift;<keycap>Z</keycap></keycombo> </shortcut> "
"<guimenu>Edit</guimenu> <guimenuitem>Redo</guimenuitem>"
msgstr ""

#. Tag: para
#: menus.docbook:198
#, no-c-format
msgid "This command <action>reverts a previous Undo</action>."
msgstr ""

#. Tag: menuchoice
#: menus.docbook:204
#, no-c-format
msgid ""
"<shortcut> <keycombo>&Ctrl;<keycap>X</keycap></keycombo> </shortcut> <guimenu>"
"Edit</guimenu> <guimenuitem>Cut</guimenuitem>"
msgstr ""

#. Tag: para
#: menus.docbook:214
#, no-c-format
msgid ""
"This command <action>removes the selected text</action>, if any, and <action>"
"places a copy of the removed text in the clipboard</action>."
msgstr ""

#. Tag: menuchoice
#: menus.docbook:220
#, no-c-format
msgid ""
"<shortcut> <keycombo>&Ctrl;<keycap>C</keycap></keycombo> </shortcut> <guimenu>"
"Edit</guimenu> <guimenuitem>Copy</guimenuitem>"
msgstr ""

#. Tag: para
#: menus.docbook:230
#, no-c-format
msgid ""
"This command <action>copies the selected text</action>"
", if any, to the clipboard."
msgstr ""

#. Tag: menuchoice
#: menus.docbook:236
#, no-c-format
msgid ""
"<shortcut> <keycombo>&Ctrl;<keycap>V</keycap></keycombo> </shortcut> <guimenu>"
"Edit</guimenu> <guimenuitem>Paste</guimenuitem>"
msgstr ""

#. Tag: para
#: menus.docbook:246
#, no-c-format
msgid ""
"This command <action>pastes the text in the clipboard</action>"
", if any, into the editor at the cursor position."
msgstr ""

#. Tag: menuchoice
#: menus.docbook:252
#, no-c-format
msgid ""
"<shortcut> <keycombo>&Ctrl;<keycap>A</keycap></keycombo> </shortcut> <guimenu>"
"Edit</guimenu> <guimenuitem>Select All</guimenuitem>"
msgstr ""

#. Tag: para
#: menus.docbook:262
#, no-c-format
msgid ""
"This command <action>selects all the entries</action> "
"in the collection, in the <interface>Column View</interface>."
msgstr ""

#. Tag: menuchoice
#: menus.docbook:268
#, no-c-format
msgid ""
"<shortcut> <keycombo>&Ctrl;&Shift;<keycap>A</keycap></keycombo> </shortcut> "
"<guimenu>Edit</guimenu> <guimenuitem>Deselect</guimenuitem>"
msgstr ""

#. Tag: para
#: menus.docbook:278
#, no-c-format
msgid ""
"This command <action>deselects all the entries</action> in the collection."
msgstr ""

#. Tag: menuchoice
#: menus.docbook:284
#, no-c-format
msgid ""
"<shortcut> <keycombo>&Ctrl;<keycap>I</keycap></keycombo> </shortcut> <guimenu>"
"Edit</guimenu> <guimenuitem>Internet Search...</guimenuitem>"
msgstr ""

#. Tag: para
#: menus.docbook:294
#, no-c-format
msgid ""
"This command <action>opens the <interface>Internet Search Dialog</interface>"
"</action> to search for and import entries from various Internet sites, "
"including <ulink url=\"http://amazon.com\">Amazon.com</ulink>."
msgstr ""

#. Tag: menuchoice
#: menus.docbook:300
#, no-c-format
msgid ""
"<shortcut> <keycombo>&Ctrl;<keycap>J</keycap></keycombo> </shortcut> <guimenu>"
"Edit</guimenu> <guimenuitem>Advanced Filter...</guimenuitem>"
msgstr ""

#. Tag: para
#: menus.docbook:310
#, no-c-format
msgid ""
"This command <action>opens the <interface>Advanced Filter Dialog</interface>"
"</action>."
msgstr ""

#. Tag: title
#: menus.docbook:321
#, no-c-format
msgid "The <guimenu>Collection</guimenu> Menu"
msgstr ""

#. Tag: menuchoice
#: menus.docbook:328
#, no-c-format
msgid ""
"<shortcut> <keycombo>&Ctrl;<keycap>N</keycap></keycombo> </shortcut> <guimenu>"
"Collection</guimenu> <guimenuitem>New Entry</guimenuitem>"
msgstr ""

#. Tag: para
#: menus.docbook:338
#, no-c-format
msgid ""
"This command <action>opens the <interface>Entry Editor</interface> "
"to edit a new entry</action>."
msgstr ""

#. Tag: menuchoice
#: menus.docbook:344
#, no-c-format
msgid ""
"<shortcut> <keycombo>&Ctrl;<keycap>E</keycap></keycombo> </shortcut> <guimenu>"
"Collection</guimenu> <guimenuitem>Edit Entry</guimenuitem>"
msgstr ""

#. Tag: para
#: menus.docbook:354
#, no-c-format
msgid ""
"This command <action>opens the <interface>Entry Editor</interface> "
"to edit the selected entries</action>."
msgstr ""

#. Tag: menuchoice
#: menus.docbook:360
#, no-c-format
msgid ""
"<shortcut> <keycombo>&Ctrl;<keycap>Y</keycap></keycombo> </shortcut> <guimenu>"
"Collection</guimenu> <guimenuitem>Duplicate Entry</guimenuitem>"
msgstr ""

#. Tag: para
#: menus.docbook:370
#, no-c-format
msgid ""
"This command <action>copies, or duplicates, the selected entries</action> "
"as new entries in the collection."
msgstr ""

#. Tag: menuchoice
#: menus.docbook:376
#, no-c-format
msgid ""
"<shortcut> <keycombo>&Ctrl;<keycap>D</keycap></keycombo> </shortcut> <guimenu>"
"Collection</guimenu> <guimenuitem>Delete Entry</guimenuitem>"
msgstr ""

#. Tag: para
#: menus.docbook:386
#, no-c-format
msgid "This command <action>deletes the selected entries</action>."
msgstr ""

#. Tag: menuchoice
#: menus.docbook:392
#, no-c-format
msgid "<guimenu>Collection</guimenu> <guimenuitem>Merge Entries</guimenuitem>"
msgstr ""

#. Tag: para
#: menus.docbook:404
#, no-c-format
msgid "This command <action>merges the selected entries</action>."
msgstr ""

#. Tag: menuchoice
#: menus.docbook:410
#, no-c-format
msgid "<guimenu>Collection</guimenu> <guimenuitem>Update Entry</guimenuitem>"
msgstr ""

#. Tag: para
#: menus.docbook:417
#, no-c-format
msgid ""
"This menu contains a list of all available data sources, and can be used to "
"automatically query the source and <action>update the selected entries</action>"
"."
msgstr ""

#. Tag: menuchoice
#: menus.docbook:424
#, no-c-format
msgid "<guimenu>Collection</guimenu> <guimenuitem>Check-out</guimenuitem>"
msgstr ""

#. Tag: para
#: menus.docbook:431
#, no-c-format
msgid ""
"This command <action>opens a dialog box for loaning the selected "
"entries.</action>."
msgstr ""

#. Tag: menuchoice
#: menus.docbook:437
#, no-c-format
msgid "<guimenu>Collection</guimenu> <guimenuitem>Check-in</guimenuitem>"
msgstr ""

#. Tag: para
#: menus.docbook:444
#, no-c-format
msgid ""
"This command <action>checks-in any of the selected entries which are currently "
"on-loan</action>."
msgstr ""

#. Tag: menuchoice
#: menus.docbook:450
#, no-c-format
msgid ""
"<shortcut> <keycombo>&Ctrl;<keycap>R</keycap></keycombo> </shortcut> <guimenu>"
"Collection</guimenu> <guimenuitem>Rename Collection</guimenuitem>"
msgstr ""

#. Tag: para
#: menus.docbook:460
#, no-c-format
msgid ""
"This command <action>opens a dialog box for renaming the collection</action>."
msgstr ""

#. Tag: menuchoice
#: menus.docbook:466
#, no-c-format
msgid ""
"<shortcut> <keycombo>&Ctrl;<keycap>U</keycap></keycombo> </shortcut> <guimenu>"
"Collection</guimenu> <guimenuitem>Collection Fields...</guimenuitem>"
msgstr ""

#. Tag: para
#: menus.docbook:476
#, no-c-format
msgid ""
"This command <action>opens the <interface>Collection Fields Dialog</interface>"
"</action>."
msgstr ""

#. Tag: menuchoice
#: menus.docbook:482
#, no-c-format
msgid ""
"<guimenu>Collection</guimenu> <guimenuitem>Generate Reports...</guimenuitem>"
msgstr ""

#. Tag: para
#: menus.docbook:489
#, no-c-format
msgid ""
"This command <action>opens the <interface>Report Dialog</interface></action> "
"for generating reports about the collection."
msgstr ""

#. Tag: menuchoice
#: menus.docbook:495
#, no-c-format
msgid ""
"<guimenu>Collection</guimenu> <guisubmenu>Bibliography</guisubmenu> "
"<guimenuitem>Convert to Bibliography</guimenuitem>"
msgstr ""

#. Tag: para
#: menus.docbook:503
#, no-c-format
msgid ""
"This command <action>converts a book collection to a bibliography</action> "
"by adding certain fields necessary for Bibtex export, and it is only enabled "
"when a book collection is open."
msgstr ""

#. Tag: menuchoice
#: menus.docbook:509
#, no-c-format
msgid ""
"<guimenu>Collection</guimenu> <guisubmenu>Bibliography</guisubmenu> "
"<guimenuitem>String Macros...</guimenuitem>"
msgstr ""

#. Tag: para
#: menus.docbook:517
#, no-c-format
msgid ""
"This command <action>opens a dialog box for editing the Bibtex string "
"macros</action> in the collection. It is only enabled when editing a "
"bibliography."
msgstr ""

#. Tag: menuchoice
#: menus.docbook:523
#, no-c-format
msgid ""
"<guimenu>Collection</guimenu> <guisubmenu>Bibliography</guisubmenu> "
"<guimenuitem>Check for Duplicate Keys</guimenuitem>"
msgstr ""

#. Tag: para
#: menus.docbook:531
#, no-c-format
msgid ""
"This command <action>checks for duplicate Bibtex keys</action> "
"and can filter the collection to show the entries with duplicated keys."
msgstr ""

#. Tag: menuchoice
#: menus.docbook:537
#, no-c-format
msgid ""
"<guimenu>Collection</guimenu> <guisubmenu>Bibliography</guisubmenu> "
"<guimenuitem>Copy Bibtex to Clipboard</guimenuitem>"
msgstr ""

#. Tag: para
#: menus.docbook:545
#, no-c-format
msgid ""
"This command <action>copies a Bibtex citation</action> "
"to the clipboard so that it can pasted in a LaTeX document."
msgstr ""

#. Tag: menuchoice
#: menus.docbook:551
#, no-c-format
msgid ""
"<guimenu>Collection</guimenu> <guisubmenu>Bibliography</guisubmenu> "
"<guimenuitem>Cite Entry in Lyx</guimenuitem>"
msgstr ""

#. Tag: para
#: menus.docbook:559
#, no-c-format
msgid ""
"This command <action>sends a citation for the selected entries</action> "
"to the so-called <emphasis><link linkend=\"hidden-bibtex-options\">"
"lyxpipe</link></emphasis> for use in <application>LyX</application>"
", <application>Kile</application>, or other LaTeX applications. It is only "
"enabled when editing a bibliography."
msgstr ""

