#, no-c-format
msgid ""
"<shortcut><keycombo action=\"simul\"> &Ctrl;<keycap>R</keycap></keycombo>"
"</shortcut> <guimenu>Game</guimenu><guimenuitem>New Round</guimenuitem>"
msgstr ""

#. Tag: para
#: index.docbook:190
#, no-c-format
msgid "Starts a new round, at the current level."
msgstr ""

#. Tag: menuchoice
#: index.docbook:194
#, no-c-format
msgid ""
"<shortcut><keycombo action=\"simul\"><keycap>P</keycap></keycombo></shortcut> "
"<guimenu>Game</guimenu><guimenuitem>Pause</guimenuitem>"
msgstr ""

#. Tag: para
#: index.docbook:197
#, no-c-format
msgid "Pauses and unpauses the game."
msgstr ""

#. Tag: menuchoice
#: index.docbook:200
#, no-c-format
msgid ""
"<shortcut> <keycombo action=\"simul\">&Ctrl; <keycap>Q</keycap></keycombo>"
"</shortcut> <guimenu>Game</guimenu><guimenuitem>Quit</guimenuitem>"
msgstr ""

