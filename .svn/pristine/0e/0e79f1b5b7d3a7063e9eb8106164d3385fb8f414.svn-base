#, no-c-format
msgid "Status"
msgstr ""

#. Tag: para
#: index.docbook:106 index.docbook:140
#, no-c-format
msgid "Enabled or Disabled."
msgstr ""

#. Tag: guilabel
#: index.docbook:109
#, no-c-format
msgid "Description"
msgstr ""

#. Tag: para
#: index.docbook:110
#, no-c-format
msgid "A description for the task, such as its purpose."
msgstr ""

#. Tag: guilabel
#: index.docbook:114
#, no-c-format
msgid "Scheduling Details"
msgstr ""

#. Tag: para
#: index.docbook:115
#, no-c-format
