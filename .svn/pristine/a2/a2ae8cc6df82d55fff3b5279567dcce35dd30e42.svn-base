#. Tag: title
#: index.docbook:76
#, no-c-format
msgid "Introduction"
msgstr ""

#. Tag: title
#: index.docbook:77
#, no-c-format
msgid "Gametype:"
msgstr ""

#. Tag: para
#: index.docbook:77
#, no-c-format
