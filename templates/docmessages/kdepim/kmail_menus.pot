#, no-c-format
msgid ""
"<action>Opens up <ulink url=\"help:/kaddressbook\">&kaddressbook;</ulink>"
"</action>."
msgstr ""

#. Tag: menuchoice
#: menus.docbook:1729
#, no-c-format
msgid ""
"<guimenu>Tools</guimenu> <guimenuitem>Save Distribution List...</guimenuitem>"
msgstr ""

