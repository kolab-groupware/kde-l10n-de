#. Tag: para
#: index.docbook:512
#, no-c-format
msgid "<guilabel>LARGE</guilabel>: even larger."
msgstr ""

#. Tag: para
#: index.docbook:513
#, no-c-format
msgid "<guilabel>huge</guilabel>: still larger."
msgstr ""

#. Tag: para
#: index.docbook:514
#, no-c-format
