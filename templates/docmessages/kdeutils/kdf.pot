#, no-c-format
msgid "<phrase>&kdiskfree; Screen</phrase>"
msgstr ""

#. Tag: para
#: index.docbook:103
#, no-c-format
msgid "an icon depicting the type of storage"
msgstr ""

#. Tag: para
#: index.docbook:104
#, no-c-format
msgid "device name"
msgstr ""

#. Tag: para
#: index.docbook:105
#, no-c-format
msgid "filesystem type"
msgstr ""

#. Tag: para
