msgstr ""

#. Tag: varname
#: index.docbook:486
#, no-c-format
msgid "MaxEnergyAddedEachRound"
msgstr ""

#. Tag: para
#: index.docbook:487
#, no-c-format
msgid "The change in maximum energy capacity at the start of each round."
msgstr ""

#. Tag: varname
#: index.docbook:490
#, no-c-format
msgid "SafeTeleportEnabled"
msgstr ""

#. Tag: para
#: index.docbook:491
#, no-c-format
msgid ""
"If set to <quote><constant>false</constant></quote> the safe teleport action "
"will be disabled."
msgstr ""

#. Tag: varname
#: index.docbook:494
#, no-c-format
msgid "CostOfSafeTeleport"
msgstr ""

#. Tag: para
#: index.docbook:495
#, no-c-format
msgid "The amount of energy consumed by teleporting safely."
msgstr ""

#. Tag: varname
#: index.docbook:498
#, no-c-format
msgid "VaporizerEnabled"
msgstr ""

#. Tag: para
#: index.docbook:499
#, no-c-format
msgid ""
"If set to <quote><constant>false</constant></quote> the vaporizer action will "
"be disabled."
msgstr ""

#. Tag: varname
#: index.docbook:502
#, no-c-format
msgid "CostOfVaporizer"
msgstr ""

#. Tag: para
#: index.docbook:503
#, no-c-format
msgid "The amount of energy consumed by using the vaporizer."
msgstr ""

#. Tag: varname
#: index.docbook:506
#, no-c-format
msgid "PushableJunkheaps"
msgstr ""

#. Tag: para
#: index.docbook:507
#, no-c-format
msgid ""
"The number of junkheaps that can be moved with a single push. Possible values "
"are <quote><constant>None</constant></quote>, <quote><constant>One</constant>"
