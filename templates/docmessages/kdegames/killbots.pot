msgstr ""

#. Tag: varname
#: index.docbook:474
#, no-c-format
msgid "EnergyAtGameStart"
msgstr ""

#. Tag: para
#: index.docbook:475
#, no-c-format
msgid ""
"The amount of energy awarded to the hero at the start of the first round."
msgstr ""

#. Tag: varname
#: index.docbook:478
#, no-c-format
msgid "EnergyAddedEachRound"
msgstr ""

#. Tag: para
#: index.docbook:479
#, no-c-format
msgid "The amount of energy awarded to the hero at the start of each round."
msgstr ""

#. Tag: varname
#: index.docbook:482
#, no-c-format
