#. Tag: guilabel
#: configuration.docbook:95
#, no-c-format
msgid "Line width:"
msgstr ""

#. Tag: para
#: configuration.docbook:97
#, no-c-format
msgid "Sets the width of the lines used for drawing the grid."
msgstr ""

#. Tag: guilabel
#: configuration.docbook:102
#, no-c-format
msgid "Tic width:"
msgstr ""

#. Tag: para
#: configuration.docbook:104
#, no-c-format
