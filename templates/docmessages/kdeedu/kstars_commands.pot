msgstr ""

#. Tag: para
#: commands.docbook:1416
#, no-c-format
msgid "Pan the display, following the drag motion."
msgstr ""

#. Tag: term
#: commands.docbook:1419
#, no-c-format
msgid "&Ctrl;+dragging the sky map"
msgstr ""

