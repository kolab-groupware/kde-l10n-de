#. Tag: title
#: man-kdeoptions.7.docbook:33
#, no-c-format
msgid "Options"
msgstr ""

#. Tag: para
#: man-kdeoptions.7.docbook:35
#, no-c-format
msgid "The following options apply to all &kde; applications:"
msgstr ""

#. Tag: term
#: man-kdeoptions.7.docbook:38
#, no-c-format
msgid "<option>--caption</option> <parameter>caption</parameter>"
msgstr ""

#. Tag: para
#: man-kdeoptions.7.docbook:39
#, no-c-format
msgid "Use <parameter>caption</parameter> as name in the titlebar."
msgstr ""

#. Tag: term
#: man-kdeoptions.7.docbook:43
#, no-c-format
msgid "<option>--icon</option> <parameter>icon</parameter>"
msgstr ""

#. Tag: para
#: man-kdeoptions.7.docbook:44
#, no-c-format
msgid "Use <parameter>icon</parameter> as the application icon."
msgstr ""

#. Tag: term
#: man-kdeoptions.7.docbook:47
#, no-c-format
msgid "<option>--config</option> <parameter>filename</parameter>"
msgstr ""

#. Tag: para
#: man-kdeoptions.7.docbook:48
#, no-c-format
msgid "Use the alternative configuration <parameter>filename</parameter>"
msgstr ""

#. Tag: option
#: man-kdeoptions.7.docbook:51
#, no-c-format
msgid "--nocrashhandler"
msgstr ""

#. Tag: para
#: man-kdeoptions.7.docbook:52
#, no-c-format
msgid "Disable the crash handler, to get core dumps."
msgstr ""

#. Tag: option
#: man-kdeoptions.7.docbook:55
#, no-c-format
msgid "--waitforwm"
msgstr ""

#. Tag: para
#: man-kdeoptions.7.docbook:56
#, no-c-format
msgid "Waits for a WM_NET compatible windowmanager."
msgstr ""

#. Tag: term
#: man-kdeoptions.7.docbook:59
#, no-c-format
msgid "<option>--style</option> <parameter>style</parameter>"
msgstr ""

#. Tag: para
#: man-kdeoptions.7.docbook:60
#, no-c-format
msgid "Sets the application &GUI; style."
msgstr ""

#. Tag: term
#: man-kdeoptions.7.docbook:63
#, no-c-format
msgid "<option>--geometry</option> <parameter>geometry</parameter>"
msgstr ""

#. Tag: para
#: man-kdeoptions.7.docbook:64
#, no-c-format
msgid "Sets the client geometry of the main widget."
msgstr ""

